{
  "Logging": {
    "path": "${basedir}/logs",
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Debug",
      "Microsoft": "Debug"
    }
  },
  "Services": {
    "Parsing": "http://192.168.0.70:7023",
    "Sentiment": "http://192.168.0.70:7017"
  },
  "Monitor": {
    "Stocks": ["AAPL", "AMD", "GOOG", "AAPL"],
    "location": "/logging/Alpha/Data",
    "Service": {
      "LongRetryDelay": 1200,
      "CallDelay": 30000,
      "LongRetryCodes": ["Forbidden"],
      "RetryCodes": [
        "Forbidden",
        "RequestTimeout",
        "InternalServerError",
        "BadGateway",
        "ServiceUnavailable",
        "GatewayTimeout"
      ],
      "MaxConcurrent": 1
    }
  }
}
